	cp XCodeSnippets/$(OBJC-PROP-COPY) $(XCODE_DIR)
	cp XCodeSnippets/$(OBJC-PROP-STRONG) $(XCODE_DIR)
	cp XCodeSnippets/$(OBJC-PROP-WEAK) $(XCODE_DIR)
	cp XCodeSnippets/$(OBJC-SETUP) $(XCODE_DIR)
