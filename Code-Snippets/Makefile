remove-obsolete-appcode:
	rm -f $(APPCODE_DIR)/qualitycoding_org.xml
	rm -f $(APPCODE_DIR)/qualitycoding_org-ObjC.xml
	rm -f $(APPCODE_DIR)/qualitycoding_org-Swift.xml

install-appcode: remove-obsolete-appcode
	mkdir -p $(APPCODE_DIR)
	cp AppCodeTemplates/qualitycoding_org\ Objective-C.xml $(APPCODE_DIR)
	cp AppCodeTemplates/qualitycoding_org\ Swift.xml $(APPCODE_DIR)
	mkdir -p $(APPCODE_EAP_DIR)
	cp AppCodeTemplates/qualitycoding_org\ Objective-C.xml $(APPCODE_EAP_DIR)
	cp AppCodeTemplates/qualitycoding_org\ Swift.xml $(APPCODE_EAP_DIR)

uninstall-appcode: remove-obsolete-appcode
	rm -f $(APPCODE_DIR)/qualitycoding_org\ Objective-C.xml
	rm -f $(APPCODE_DIR)/qualitycoding_org\ Swift.xml
	rm -f $(APPCODE_EAP_DIR)/qualitycoding_org\ Objective-C.xml
	rm -f $(APPCODE_EAP_DIR)/qualitycoding_org\ Swift.xml

remove-obsolete-xcode:
	rm -f $(XCODE_DIR)/$(OBJC-HAMCREST)
	rm -f $(XCODE_DIR)/$(OBJC-HAMCRESTIOS)
	rm -f $(XCODE_DIR)/$(OBJC-MOCKITO)
	rm -f $(XCODE_DIR)/$(OBJC-MOCKITOIOS)
	rm -f $(XCODE_DIR)/$(OBJC-TESTBUTTON)
	rm -f $(XCODE_DIR)/$(OBJC-TESTBUTTONX)
	rm -f $(XCODE_DIR)/$(OBJC-TEST-OUTLETS-HAMCREST)
	rm -f $(XCODE_DIR)/$(SWIFT-HAMCREST)
	rm -f $(XCODE_DIR)/$(SWIFT-TESTBUTTON)
	rm -f $(XCODE_DIR)/$(SWIFT-TESTBUTTONX)
	rm -f $(XCODE_DIR)/$(SWIFT-TESTOUTLET)
	rm -f $(XCODE_DIR)/$(SWIFT-UNOWNEDSELF)
	rm -f $(XCODE_DIR)/$(SWIFT-WEAKSELF)

install-xcode: remove-obsolete-xcode
	mkdir -p $(XCODE_DIR)
	cp XCodeSnippets/$(OBJC-ACTION) $(XCODE_DIR)
	cp XCodeSnippets/$(OBJC-ASSERT-NOT-NIL) $(XCODE_DIR)
