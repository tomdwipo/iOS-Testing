	cp XCodeSnippets/$(SWIFT-ACTION) $(XCODE_DIR)
	cp XCodeSnippets/$(SWIFT-ASSERT-NOT-NIL) $(XCODE_DIR)
	cp XCodeSnippets/$(SWIFT-DISABLE) $(XCODE_DIR)
