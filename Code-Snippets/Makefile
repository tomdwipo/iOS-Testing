	cp XCodeSnippets/$(OBJC-TEST) $(XCODE_DIR)
	cp XCodeSnippets/$(OBJC-TEST-BUTTON-TAP) $(XCODE_DIR)
	cp XCodeSnippets/$(OBJC-TEST-OUTLETS-XCTEST) $(XCODE_DIR)
